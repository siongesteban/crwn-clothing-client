{
  "extends": [
    "react-redux-typescript-scripts/tslint.json",
    "react-redux-typescript-scripts/tslint-react.json"
  ],
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}
